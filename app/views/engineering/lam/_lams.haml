%table
	%tr
		%th Time Ago
		%th Serial #
		%th Commit Date
		%th Commit ID
		%th Modified?
		%th Power Reset?
		%th Ext Reset?
		%th Brown-out Reset?
		%th Watch-dog Reset?
	-@lams.each do |l|
		%tr
			%td&=time_ago_in_words l.created_at, true
			%td&=format_serialNumber l.serialNumber
			%td&=l.commitDate
			%td=link_to_commitID(l)
			%td= styled l.modified,l.modified
			%td= styled l.powerReset,l.powerReset
			%td= styled l.extReset,l.extReset
			%td= styled l.brownoutReset,l.brownoutReset
			%td= styled l.wdogReset,l.wdogReset
