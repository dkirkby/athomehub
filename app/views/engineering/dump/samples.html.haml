-content_for :head do
	=javascript_include_tag 'excanvas','flot','dump-plot'

%h1 Engineering::Dump#samples

%h2= "Source: #{source_label @dump}"

%h2= "Timestamp: #{@dump.micros} &micro;s"

%script= "plotData = #{@data.to_json};"

%table
	%tr
		%th Offset (&micro;s)
		%th ADC Value
	-@data.each do |row|
		%tr
			%td&= row[0]
			%td&= row[1]
